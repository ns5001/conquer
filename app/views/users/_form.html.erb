
<%= form_for user do |f| %>

  <%= f.label :"Name:" %>
  <%= f.text_field :name, :placeholder => "Name"%><br><br>

  <%= f.label :"Age:" %>
  <%= f.text_field :age %><br><br>

  <%= f.label :"Weight:" %>
  <%= f.text_field :weight %><br><br>

  <%= f.label :"Height:" %>
  <%= f.text_field :height %><br><br>

  <%= f.label :"Gender:" %>
  <%= f.text_field :gender %><br><br>


  <%= f.label :"Bio:" %>
  <%= f.text_area :bio, size: "50x3" %><br><br>


  <%= f.submit "Edit Profile" %>

  <% end %>
